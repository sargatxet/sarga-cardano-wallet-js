    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.11.5",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "cardano-addresses": "^3.8.0",
    "cd": "^0.3.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
